<?xml version="1.0" encoding="utf-8"?>
<csr.dmt.zust.edu.cn.funjobapplication.module.layout.SquareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sf_image"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/app_name"
        android:scaleType="centerCrop" />

    <ImageView
        android:id="@+id/iv_selected"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_gravity="top|end"
        android:src="@drawable/ic_check_background" />
</csr.dmt.zust.edu.cn.funjobapplication.module.layout.SquareFrameLayout>